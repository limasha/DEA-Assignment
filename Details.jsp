        <center><h1 style="color:MediumSeaGreen;">Verifying Details</h1><br><br> 
        <%-- Include the ValidateUser.java class whose method  
             boolean validate(String, String) we will be using 
        --%> 
        <%-- Create and instantiate a bean and assign an id to  
             uniquely identify the action element throughout the jsp 
        --%> 
        
        <jsp:useBean id="user" class="Beans.Checkout"/> 
          
        <%-- Set the value of the created bean using form data --%> 
        <jsp:setProperty name="user" property="name"/> 
        <jsp:setProperty name="user" property="company"/>
        <jsp:setProperty name="user" property="address"/>
        <jsp:setProperty name="user" property="address2"/>
        <jsp:setProperty name="user" property="city"/>
        <jsp:setProperty name="user" property="state1"/>
        <jsp:setProperty name="user" property="zip"/>
        <jsp:setProperty name="user" property="email"/>
        <jsp:setProperty name="user" property="phone"/>
          
        <%-- Display the form data --%> 
       <h3> The Details Entered Are as Under</h3>
        <br/> 
        <br>
        <p>User Name : <%=user.getName() %></p>
        <p>Company : <%=user.getCompany() %></p>
        <p>Address : <%=user.getAddress() %></p>
        <p>Address2 :<%=user.getAddress2() %></p>
        <p>city : <%=user.getCity() %></p>
        <p>state1 :<%=user.getState1() %></p>
        <p>zip : <%=user.getZip() %></p>
        <p>email :<%=user.getEmail() %></p>
        <p>phone : <%=user.getPhone() %></p>
       
        </center>
